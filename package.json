{
  "name": "octofiles",
  "version": "0.0.1",
  "description": "> Easily upload files to GitHub via Chrome Extension",
  "main": "app.js",
  "scripts": {
    "build": "git archive -o ./dist/build-$npm_package_version.zip HEAD",
    "release": "source .env && webstore upload --source=./dist/build-$npm_package_version --auto-publish --extension-id $EXTENSION_ID --client-id $CLIENT_ID --client-secret $CLIENT_SECRET --refresh-token $REFRESH_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darcyclarke/octofiles.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darcyclarke/octofiles/issues"
  },
  "homepage": "https://github.com/darcyclarke/octofiles#readme",
  "devDependencies": {
    "chrome-webstore-upload-cli": "^1.2.0"
  }
}
