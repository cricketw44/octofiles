{
  "name": "octofiles",
  "version": "0.1.1",
  "description": "> Easily upload files to GitHub via Chrome Extension",
  "main": "app.js",
  "scripts": {
    "set-version": "node ./scripts/set-manifest-version.js",
    "compress": "zip -r ./dist/build-$npm_package_version.zip . -x '.*' './node_modules/*' './dist/*' './scripts/*'",
    "build": "npm run set-version && npm run compress",
    "publish": "source .env && webstore upload --source=./dist/build-$npm_package_version.zip --auto-publish --extension-id $EXTENSION_ID --client-id $CLIENT_ID --client-secret $CLIENT_SECRET --refresh-token $REFRESH_TOKEN",
    "release": "npm run build && np && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darcyclarke/octofiles.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darcyclarke/octofiles/issues"
  },
  "homepage": "https://github.com/darcyclarke/octofiles#readme",
  "devDependencies": {
    "chrome-webstore-upload-cli": "^1.2.0"
  }
}
